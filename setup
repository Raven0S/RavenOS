fs.makeDir("os")
fs.makeDir("boot")
fs.makeDir("cmds")
fs.makeDir("user")

function install(addr, filename)
  shell.run("wget "..addr.." "..filename)
  term.clear()
  term.setCursorPos(1,1)
  print("Installing... "..filename)
end

term.clear()
term.setCursorPos(1,1)
install("https://raw.githubusercontent.com/Raven0S/RavenOS/master/startup", "startup")
